# Ignore environment files
.env
env/*.env

# Ignore secret keys
secrets/
*.key
*.pem

# Ignore Docker files that shouldn't be tracked
**/docker-compose.override.yml
**/docker-compose.local.yml

# Ignore OS-generated files
.DS_Store
Thumbs.db

# Ignore logs
*.log
logs/
*.out

# Ignore build artifacts
*.tar
*.gz
*.zip

# Ignore Python cache if you use scripts
__pycache__/
*.pyc

# Ignore SSH known_hosts (if used locally)
.ssh/known_hosts

# Node (in case you install web UIs)
node_modules/
dist/

# Terraform (if you later add IaC)
*.tfstate
*.tfstate.backup
.terraform/
crash.log

# GitHub Codespaces or DevContainers
.devcontainer/
.vscode/